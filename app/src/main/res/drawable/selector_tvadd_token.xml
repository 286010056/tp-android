<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="5dp" />
            <stroke android:color="@color/tab_text_color_unselected" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="5dp" />
            <stroke android:color="@color/tab_text_color_selected" />
        </shape>
    </item>
</selector>